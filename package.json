{
  "name": "twin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:seed": "ts-node ./prisma/seed.ts",
    "db:reset": "npx prisma db push --force-reset && npx prisma db push",
    "server:dev": "npm run db:generate &&  npm run db:reset && tsx --watch src/infrastructure/http/main.ts",
    "server:run": "tsx src/infrastructure/http/main.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prisma": {
    "schema": "./src/infrastructure/db/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.4",
    "@types/nodemailer": "^6.4.8",
    "@types/uuid": "^9.0.1",
    "@vitest/coverage-c8": "^0.31.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "@types/bcrypt": "^5.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/mongoose": "^5.11.97",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.2",
    "uuid": "^9.0.0"
  }
}
